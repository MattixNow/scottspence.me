{
  "name": "scottspence.me",
  "description": "Gatsby default starter",
  "version": "0.1.0",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.153",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "react-helmet": "^5.2.0"
  },
  "keywords": ["gatsby", "portfolio"],
  "license": "MIT",
  "main": "n/a",
  "now": {
    "name": "scottportfolio"
  },
  "scripts": {
    "clean":
      "rm -rf .cache/ && rm -rf scottportfolio/ && rm -rf public/",
    "build": "gatsby build && mv public scottportfolio",
    "dev": "npm run clean && gatsby develop",
    "format":
      "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.10.2"
  }
}
